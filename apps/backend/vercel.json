{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/healthz",
      "destination": "/api/healthz"
    },
    {
      "source": "/admin",
      "destination": "/api/admin"
    },
    {
      "source": "/admin/:path*",
      "destination": "/api/admin/:path*"
    },
    {
      "source": "/vibbit/:path*",
      "destination": "/api/vibbit/:path*"
    }
  ]
}
